	<%= options_for_select(StageMobility::MOBILITY,"main") %>
	</select>
</div>
<div class="span3">
	<label>Residency</label>
	<select id = "stage_residency_residency">
	<option value="main" selected>Choose a type of Residency</option>
	<%= options_for_select(StageResidency::RESIDENCY,"main") %>
	</select>
</div>
<div class="span3">
	<label>Residency Time</label>
	<input type="number" id="stage_residency_time_residency_time">
</div>
<div class="span3">
